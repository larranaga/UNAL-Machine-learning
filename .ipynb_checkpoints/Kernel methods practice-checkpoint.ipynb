    "import numpy as np\n",
    "\n",
    "\n",
    "def gaussian_kernel(x, l, sigma):\n",
    "    num = ((x - l).multiply(x - l)).dot(np.ones(x.shape[0]))\n",
    "    return np.power(np.exp( (1.0/(2*sigma**2)) * num ))\n"
   ]
